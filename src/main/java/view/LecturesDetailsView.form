<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="view.LecturesDetailsView">
  <grid id="27dc6" binding="mainPane" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties>
      <preferredSize width="800" height="300"/>
    </properties>
    <border type="none"/>
    <children>
      <grid id="55593" binding="buttonPane" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="West"/>
        <properties>
          <preferredSize width="150" height="300"/>
          <verifyInputWhenFocusTarget value="true"/>
        </properties>
        <border type="none"/>
        <children>
          <component id="75a00" class="javax.swing.JButton" binding="joinDropLectureButton">
            <constraints border-constraint="North"/>
            <properties>
              <text value="Join Lecture"/>
              <toolTipText value="Click here to join the selected lecture"/>
            </properties>
          </component>
          <component id="1a13c" class="javax.swing.JButton" binding="mapLocationButton">
            <constraints border-constraint="Center"/>
            <properties>
              <text value="Map"/>
            </properties>
          </component>
          <component id="8df36" class="javax.swing.JButton" binding="participantsButton">
            <constraints border-constraint="South"/>
            <properties>
              <text value="Participants"/>
            </properties>
          </component>
        </children>
      </grid>
      <grid id="aea3f" binding="commentPane" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="East"/>
        <properties>
          <preferredSize width="400" height="300"/>
        </properties>
        <border type="none"/>
        <children/>
      </grid>
      <grid id="8404c" binding="detailsPane" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="Center"/>
        <properties>
          <preferredSize width="250" height="300"/>
        </properties>
        <border type="none"/>
        <children>
          <component id="eb406" class="javax.swing.JTextPane" binding="textPane">
            <constraints border-constraint="Center"/>
            <properties/>
          </component>
        </children>
      </grid>
    </children>
  </grid>
</form>
