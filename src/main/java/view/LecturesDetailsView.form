<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="view.LecturesDetailsView">
  <grid id="27dc6" binding="mainPane" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties>
      <preferredSize width="0" height="100"/>
    </properties>
    <border type="none"/>
    <children>
      <grid id="55593" binding="buttonPane" layout-manager="FormLayout">
        <rowspec value="center:d:noGrow"/>
        <rowspec value="top:4dlu:noGrow"/>
        <rowspec value="center:max(d;4px):noGrow"/>
        <rowspec value="top:4dlu:noGrow"/>
        <rowspec value="center:max(d;4px):noGrow"/>
        <colspec value="fill:d:grow"/>
        <constraints border-constraint="West"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="75a00" class="javax.swing.JButton" binding="joinDropLectureButton">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="1" fill="0" indent="0" use-parent-layout="false"/>
              <forms/>
            </constraints>
            <properties>
              <hideActionText value="false"/>
              <text value="Join Lecture"/>
              <toolTipText value="Click here to join the selected lecture"/>
            </properties>
          </component>
          <component id="7f21a" class="javax.swing.JButton" binding="mapLocationButton">
            <constraints>
              <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
              <forms/>
            </constraints>
            <properties>
              <text value="Map"/>
            </properties>
          </component>
          <component id="41a6d" class="javax.swing.JButton" binding="participantsButton" default-binding="true">
            <constraints>
              <grid row="4" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
              <forms/>
            </constraints>
            <properties>
              <text value="Participants"/>
            </properties>
          </component>
        </children>
      </grid>
      <grid id="aea3f" binding="commentPane" layout-manager="GridLayoutManager" row-count="1" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
        <margin top="0" left="0" bottom="0" right="0"/>
        <constraints border-constraint="East"/>
        <properties/>
        <border type="none"/>
        <children/>
      </grid>
      <grid id="8404c" binding="detailsPane" layout-manager="GridLayoutManager" row-count="1" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
        <margin top="0" left="0" bottom="0" right="0"/>
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="ca8ad" class="javax.swing.JTextPane" binding="textPane">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="6" hsize-policy="6" anchor="0" fill="3" indent="0" use-parent-layout="false">
                <preferred-size width="150" height="50"/>
              </grid>
            </constraints>
            <properties/>
          </component>
        </children>
      </grid>
    </children>
  </grid>
</form>
