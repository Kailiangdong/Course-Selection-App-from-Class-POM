<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="view.LecturesDetailsView">
  <grid id="27dc6" binding="mainPane" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties>
      <preferredSize width="0" height="100"/>
    </properties>
    <border type="none"/>
    <children>
      <component id="75a00" class="javax.swing.JButton" binding="joinLectureButton">
        <constraints border-constraint="West"/>
        <properties>
          <text value="Join Lecture"/>
          <toolTipText value="Click here to join the selected lecture"/>
        </properties>
      </component>
      <component id="2240" class="javax.swing.JButton" binding="dropLectureButton">
        <constraints border-constraint="East"/>
        <properties>
          <text value="Drop Lecture"/>
          <toolTipText value="Click here to drop the selected lecture"/>
        </properties>
      </component>
    </children>
  </grid>
</form>
